.container {
    background-color: #eee;
    height: 100%;
}
/* 头部信息开始 */
.home_top {
    width: 100%;
    height: 404rpx;
    color: #fff;
    background: -webkit-linear-gradient(left,#F43A2A,#FFA224)
}
.home_top-pro_photo image {
    margin: 40rpx 20rpx 0 40rpx;
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
}
.home_top-light {
    margin-top: 66rpx;
}
.home_top-light .name {
    font-size: 34rpx;
    margin-right: 22rpx;
}
.home_top-light .the-title {
    margin-right: 120rpx;
    width: 130rpx;
    height: 38rpx;
    line-height: 38rpx;
    text-align: center;
    background: #FBA885;
    border-radius: 10rpx;
}
.contact {
    position: relative;
    top: -116rpx;
    width: 100%;
}
.contact-clud {
    padding-top: 30rpx;
    border-radius: 20rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
    height: 200rpx;
    background: #fff;
}
.contact image {
    width: 84rpx;
    height: 70rpx;
}
.contact .line {
    width: 4rpx;
    height: 140rpx;
    margin-top: 20rpx;
    margin-left: 80rpx;
    background: #EFEFEF;
}
.contact .top-phone {
    margin-left: 80rpx;
}
/* 头部信息结束 */
/* 导航菜单开始 */
.nav {
    position: relative;
    top: -40rpx;
    width: 100%;
    margin-bottom: 80rpx;
}
.nav-tabs {
    margin-left: 20rpx;
    margin-right: 20rpx;
    height: 540rpx;
    background: #fff;
}
.nav-top {
    width: 100%;
   padding-top: 40rpx;
   padding-bottom: 40rpx;
   border-bottom: 2rpx solid #EFEFEF;
}
.border-none {
    border: none;
}
.nav-top .m-left {
    margin-left: 40rpx;
    /* margin-right: 80rpx; */
}
.nav-top text {
    color: #1E1E1E;
    font-size: 30rpx;
}
.nav-tabs .lines {
    width: 4rpx;
    height: 100rpx;
    margin-top: 30rpx;
    margin-left: 68rpx;
    margin-right: 68rpx;
    background-color: #EFEFEF;
}
.nav image {
    margin-bottom: 24rpx;
    width: 120rpx;
    height: 120rpx;
}
/* 导航菜单结束 */