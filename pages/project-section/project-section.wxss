    font-size: 32rpx;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    border-radius: 10rpx;
    color: #F54342;
    border: 2rpx solid #F54342;
}
.list-col {
    margin-bottom: 40rpx;
}
.swiper {
    height: 800rpx;
}