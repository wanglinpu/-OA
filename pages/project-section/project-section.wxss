.project-list .request {
    height: 100rpx;
    line-height: 112rpx;
}
.project-list .request image {
    position: relative;
    top: 10rpx;
    margin: 0;
    margin-right: 20rpx;
    width: 52rpx;
    height: 52rpx;
}
.project-list .request text {
    font-size: 32rpx;
    color: #202020;
}
