/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 28rpx;
  justify-content: space-between;
  box-sizing: border-box;
} 
image {
  width: 30rpx;
  height: 30rpx;
}
.flex-wrp-row {
  /* width: 100%; */
  display: flex;
  flex-direction: row;
}
.flex-wrp-col {
  /* width: 100%; */
  display: flex;
  flex-direction: column;
}
/* 我的款项开始 */
page {
  background: #F0F1F3;
  font-family: PingFang-SC-Medium;
}

.col-ctn {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  padding-right: 20rpx;   
  box-sizing: border-box;
}
.collection {
  width: 100%;
  background: #FFF;
  box-sizing: border-box;
  padding-bottom: 20rpx;
}
.head-photo {
  text-align: center;
}
.head-photo image{
  margin: 0 auto;
  color: red;
  margin-top: 20rpx;
  width: 70rpx;
  height: 70rpx;
}
.head-photo text {
  margin: 0 auto;
  margin-top: 20rpx;
}
.collection-details {
  margin: 40rpx 0;
}
.collection-details view {
  font-size: 30rpx;
  margin-left: 34rpx;
  margin-right: 34rpx;
  margin-bottom: 20rpx;
}
.collection-details text {
  color: #999;
  
  height: 70rpx;
  line-height: 70rpx;
}
.collection-details input {
  height: 70rpx;
  width: 456rpx;
  line-height: 70rpx;
  margin-left: 40rpx;
  border: 1px solid #F0F1F3;
}

.digest textarea {
  padding-left: 120rpx;
  padding-top: 20rpx;
  width: 100%;
  height: 290rpx;
  background: #F2F2F2;
  margin-right: 20rpx;
  box-sizing: border-box;
}
.digest image {
  position: relative;
  top: 0rpx;
  left: -80rpx;
  width: 40rpx;
  height: 40rpx;
}
.check-box text{
  width: 200rpx;
  height: 60rpx;
  border-radius: 10rpx;
  line-height: 60rpx;
  border: 4rpx solid red;
  color: red;
  text-align: center;
  margin-right: 60rpx;
}
.check-box .checked_parameter{
  color: #fff;
  background: red;
}
.section {
  position: relative;
}
.section picker {
  height: 70rpx;
  width: 456rpx;
  color: #999;
  line-height: 70rpx;
  margin-left: 40rpx;
  border: 2rpx solid #F0F1F3;
}
.section image {
  position: absolute;
  top: 20rpx;
  left: 500rpx;
  width: 50rpx;
  height: 40rpx;
}
.collection-details .add-attachment image {
  position: static;
  width: 180rpx;
  height: 180rpx;
  border: 4rpx dashed #F0F1F3; 
}
.collection-details .add-attachment view {
 padding-left: 0;
}
.collection-details .approval-people image {
   position: absolute;
   left: -40rpx;
  width: 90rpx;
  height: 90rpx;
}
.check-box {
  position: relative;
  margin-bottom: 40rpx;
  height: 200rpx;
}
.collection-details .approval-people .name {
   position: absolute;
   border: none;
   color: #000;
   font-size: 28rpx;
   left: -100rpx;
   top: 80rpx;
}
.collection-details .del image {
   position: absolute;
   left: 18rpx;
   background: #555451;
   width: 30rpx;
   height: 30rpx;
}
.check-box image {
   margin-right: 80rpx;
   width: 100rpx;
   height: 100rpx;
}
.collection-details  .copy-to {
   margin-bottom: 100rpx;
}
.copy-to image {
   width: 100rpx;
   height: 100rpx;
}
/* 提交 */
.submit {
  position: fixed;
  bottom: 20rpx;
  width: 100%;
}
.submit navigator {
  margin-top: 100rpx;
  margin: 0 auto;
  /* position: fixed; */
  color: #fff;
  height: 74rpx;
  line-height: 74rpx;
  font-size: 32rpx;
  border-radius: 20rpx;
  text-align: center;
  width: 600rpx;
  background: #F54342;
}
/* 我的款项结束 */
.contact {
  position: relative;
  top: -60rpx;
  width: 100%;
}
.contact-clud {
  padding-top: 30rpx;
  border-radius: 20rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  /* height: 2100%; */
  background: #fff;
}
.contact image {
  width: 84rpx;
  height: 70rpx;
}




.customer-details {
  width: 100%;
  padding: 20rpx;
  padding-bottom: 40rpx;
  box-sizing: border-box;
}
.customer-box {
  width: 100%;
  height: 100%;
  background: #fff;
  padding: 0 20rpx;
  padding-bottom: 40rpx;
  box-sizing: border-box;
}
.customer-bar {
  width: 100%;
  height: 106rpx;
  line-height: 106rpx;
  font-size: 34rpx;
  left: 106rpx;
  border-bottom: 1px solid #ddd;
  box-sizing: border-box;

}
.customer-bar .call {
  position: relative;
  color: #999;
}
.customer-bar .name {
  position: absolute;
  color: #1F1F1F;
  right: 40rpx;
  /* right: -500rpx; */
}
.digest {
  width: 100%;
  box-sizing: border-box;
  margin-top:40rpx;
}
.digest text {
  color: #999;
  font-size: 34rpx;
  margin-top: 20rpx;
}
.reject textarea {
  padding-left: 0;
  color: #868686;
}
.adjunct {
  width: 100%;
}
.adjunct text {
  color: #999;
  margin-bottom: 20rpx;
  font-size: 34rpx;
  margin-top: 40rpx;
}
.adjunct imgs {
  width: 100%;
}
.adjunct image {
 width: 320rpx;
 height: 226rpx;
}
.adjunct .mr20 {
  margin-right: 24rpx;
}
/* 流程状态 */
.process {
  margin-top: 70rpx;
  width: 100%;
  /* height: 100%; */
}
.process-top {
  width: 100%;
  color: #1F1F1F;
  text-align: center;
  margin-bottom: 40rpx;
}
.process-top .line {
  margin: 0 auto;
  width: 70rpx;
  border-radius: 10rpx;
  height: 10rpx;
  background: #ddd;
}
.process-top .title {
  font-size: 40rpx;
  margin: 20rpx;
  font-weight: bold;
}
.process-top .times {
  font-size: 32rpx;
  height: 40rpx;
}
.process .shape image {
  margin-top: 40rpx;
  width: 50rpx;
  height: 50rpx;
}
.process .state {
  position: relative;
  width: 100%;
  height: 160rpx;
  padding-top: 10rpx;
  padding-left: 20rpx;
  padding-bottom: 10rpx;
  background: #F9F9F9;
  margin-left: 26rpx;
  box-sizing: border-box;
}
.process-buttom  .dashed {
  position: absolute;
  left: 24rpx;
  top: 90rpx;
  width: 4rpx;
  height: 156rpx;
  border-left: 4rpx dashed #ddd;
}
.process .state image {
  /* margin: 20rpx 0rpx 0 40rpx; */
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.process-buttom {
  position: relative;
  margin-bottom: 46rpx;
}
.process-buttom .state-btm {
  margin-left: 20rpx;
}
.process-buttom .state-btm-top {
  position: relative;
}
.process-buttom .state-btm .names {
  color: #1F1F1F;
  font-size: 28rpx;
}
.process-buttom .state-btm-top .time {
  position: absolute;
  right: 0;
  color: #BABABA;
}
.process-buttom .state-btm .launch {
  color: #F06666;
  font-size: 32rpx;
  margin: 20rpx 0;
}
.process .explain {
  overflow : hidden;
  color: #BABABA;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

